{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit --incremental false",
    "lint": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --cache --cache-strategy content --cache-location=node_modules/.cache/eslint/.eslint-cache src",
    "format": "prettier --check --log-level warn --cache --cache-strategy content '**/*.{js,ts,tsx,css}'",
    "format:write": "prettier --write --log-level warn --cache --cache-strategy content '**/*.{js,ts,tsx,css}'",
    "check-all": "npm run typecheck && npm run lint && npm run format",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "js-cookie": "^3.0.5",
    "next": "14.0.4",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/types": "^18.4.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3"
  }
}
